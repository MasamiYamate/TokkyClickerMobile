var count=0,win1=Ti.UI.createWindow({title:"Tab 1",backgroundColor:"#000000",fullscreen:!0}),label1=Ti.UI.createLabel({color:"#FFFFFF",font:{fontSize:38,fontFamily:"Helvetica Neue"},textAlign:"center",width:"auto",top:400}),tokkybutton=Ti.UI.createButton({backgroundImage:"photo/tokinotoki.png",height:200,width:200});tokkybutton.addEventListener("singletap",function(){return count+=1,Ti.API.info(count),label1.text="click:"+count,count}),win1.add(tokkybutton),win1.add(label1),win1.open();